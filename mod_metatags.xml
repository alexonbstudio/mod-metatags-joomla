<?xml version="1.0" encoding="utf-8"?><extension type="module" version="3.9" client="site" method="upgrade">	<name>mod_metatags</name>	<author>Alexon Balangue</author>	<creationDate>19 april 2019</creationDate>	<copyright>Copyright (C) 2012-2020. All rights reserved.</copyright>	<license>http://www.gnu.org/licenses/gpl-3.0.html GNU/GPL</license>	<authorEmail>contact@alexonbstudio.fr</authorEmail>	<authorUrl>www.alexonbstudio.fr</authorUrl>	<version>4.3</version>	<description>MOD_METATAGS_XML_DESCRIPTION</description>	<files>		<filename module="mod_metatags">mod_metatags.php</filename>		<filename>mod_metatags.xml</filename>		<folder>tmpl</folder>		<folder>language</folder>		<filename>index.html</filename>		</files>	<languages folder="language">		<language tag="en-GB">en-GB/en-GB.mod_metatags.ini</language>		<language tag="fr-FR">fr-FR/fr-FR.mod_metatags.ini</language>		<language tag="en-GB">en-GB/en-GB.mod_metatags.sys.ini</language>		<language tag="fr-FR">fr-FR/fr-FR.mod_metatags.sys.ini</language>	</languages>		<config>		<fields name="params">			<fieldset name="basic" label="Basic">				<field 					name="robots"					type="list"					default="index, follow"					label="robots"					description="">						<option value="">JFIELD_MOD_METATAGS_CLASSIFICATION_OPTION_0</option>						<option value="index, follow">index, follow</option>						<option value="noindex, follow">noindex, follow</option>						<option value="index, nofollow">index, nofollow</option>						<option value="noindex, nofollow">noindex, nofollow</option>				</field>				<field 					type="media"					name="logo"					default=""					label="Logo"					description="" 					directory="" />			</fieldset>			<fieldset name="twitter" label="Twitter Cards">				<field 					name="twitter-card"					type="list"					default="summary"					label="Twitter Card"					description="" >						<option value="">JFIELD_MOD_METATAGS_CLASSIFICATION_OPTION_0</option>						<option value="summary">Summary</option>						<option value="summary_large_image">Summary Large image</option>				</field>								<field 					type="text"					name="twitter-user"					label="username"					default="alexonbstudio"					description="" />			</fieldset>			<fieldset name="open-graph" label="Open Graph &amp; Facebook">				<field 					name="og-type"					type="list"					default="article"					label="JFIELD_MOD_METATAGS_OGTYPE_LABEL"					description="JFIELD_MOD_METATAGS_OGTYPE_DESC">						<option value="">JFIELD_MOD_METATAGS_CLASSIFICATION_OPTION_0</option>						<option value="website">website</option>						<option value="article">article</option>				</field>				<field 					name="fb-admins"					type="text"					default=""					label="JFIELD_MOD_METATAGS_FBADMIN_LABEL"					description="JFIELD_MOD_METATAGS_FBADMIN_DESC" />				<field 					name="fb-app-id"					type="text"					default=""					label="JFIELD_MOD_METATAGS_FBAPP_LABEL"					description="JFIELD_MOD_METATAGS_FBAPP_DESC" />				<field 					name="fb-profils-id"					type="text"					default=""					label="MOD_METATAGS_FBPROFILSID_LABEL"					description="MOD_METATAGS_FBPROFILSID_DESC" />			</fieldset>						<fieldset name="mod_meta-backlinks" label="Custom script &amp; Backlinks">				<field 					name="head-script-frontend" 					type="textarea" 					default="" 					label="Enter some html (HEAD)" 					description="" 					rows="10" cols="5" 					filter="raw"/>				<field 					name="footer-script-frontend" 					type="textarea" 					default="" 					label="Enter some html (BODY)" 					description="" 					rows="10" cols="5" 					filter="raw"/>				</fieldset>			<fieldset name="json-ld" label="JSON-LD ">					<field 					name="jld-type"					type="list"					default="json-ld-person"					label="Json-LD - Type"					description="">						<option value="">MOD_METATAGS_CHOOSE</option>						<option value="json-ld-person">Person</option>						<option value="json-ld-organisation">Organisation</option>				</field>			</fieldset>						<fieldset name="analystics-marketing" label="Analystic &amp; Marketing">								<field 					name="g-tagmanger"					type="text"					default=""					label="Google Tags Manager"					description="api Code GTM" />				<field 					name="g-analytics"					type="text"					default=""					label="Google Analystics"					description="api: UA-xxxx-x" />					<field 					name="g-adsense"					type="text"					default=""					label="Google Adsense"					description="api ca-xxxxxxxx " />				<field 					name="yx-analytics"					type="text"					default=""					label="Yandex Analystics API;xxxxxxx"					description="" />					<field 					name="m-33across"					type="text"					default=""					label="33Across"					description="API;xxxxxxx" />					<field 					name="m-awin"					type="text"					default=""					label="Awin"					description="API;xxxxxxx" />					<field 					name="m-uiz"					type="text"					default=""					label="UIZ"					description="Short link auto API;xxxxxxx" />					<field 					name="m-quantcast"					type="text"					default=""					label="Quantcast"					description="API;xxxxxxx" />				</fieldset>			<fieldset name="shares-social-media" label="Shares Social Networks">					<field 					name="s-shareaholic"					type="text"					default=""					label="shareaholic"					description="Site ID API;xxxxxxx" />						<field 					name="s-addthis"					type="text"					default=""					label="addthis"					description="Site ID API;xxxxxxx" />				</fieldset>			<fieldset name="chatbot" label="Chatbot">					<field 					name="crisp"					type="text"					default=""					label="crisp"					description="API;xxxxxxx" />						<field 					name="tidio"					type="text"					default=""					label="addthis"					description="API;xxxxxxx" />				</fieldset>		</fields>	</config></extension>